{
  "root": true,
  "extends": ["airbnb-base",  "prettier"],
  "env": {
    "jest": true,
    "jest/globals": true

  },
  "plugins": [ "jest", "prettier" ],
  "rules": {
    "prettier/prettier": "error",
    "no-multi-str": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-tabs": 2,
    "padded-blocks": [2, {
      "blocks": "never",
      "classes": "always",
      "switches": "never"
    }],
    "no-console": 0,
    "max-len": 0
  }
}
